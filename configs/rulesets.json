                "seed": 12345,
                "criteria": {}
              }

            },
            {
              "id": 1,
              "name": "Lowest Distance",
              "description": "Bevorzuge den Auftrag der am dichtesten ist.",
              "rules":
              {
                "criteria": {
                  "numerical": [
                    {
                      "measure": "distance",
                      "ranking_order": "ASC",
                      "weight": 1
                    }
                  ]
                }
              }

            },
            {
